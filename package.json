{
  "name": "store-design",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">= 8.10.0",
    "npm": ">= 5.6.0"
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "collectCoverageFrom": [
      "<rootDir>/src/**/*.tsx",
      "<rootDir>/src/**/*.ts",
      "<rootDir>/src/**/*.js",
      "<rootDir>/src/**/*.jsx"
    ]
  },
  "dependencies": {
    "axios": "^0.18.0",
    "bc-react-scripts": "git+ssh://git@github.com:bigcommerce-labs/bc-react-scripts.git#v1.0.0-alpha.1",
    "js-cookie": "^2.2.0",
    "jschannel": "bigcommerce-labs/jschannel",
    "jsonp": "^0.2.1",
    "lodash": "^4.17.10",
    "query-string": "^5.1.1",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "styled-components": "^3.3.3",
    "tinycolor2": "^1.4.1",
    "unescape": "^1.0.1",
    "uuid": "^3.2.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "dev": "react-scripts dev",
    "test": "react-scripts test --env=jsdom",
    "test:coverage": "react-scripts test --env=jsdom --coverage",
    "test:updateSnapshot": "react-scripts test --updateSnapshot --env=jsdom",
    "eject": "react-scripts eject",
    "travis": "react-scripts travis",
    "eslint": "eslint src --ext .js,.jsx,.ts,.tsx"
  },
  "homepage": "https://github.com/bigcommerce/store-design",
  "devDependencies": {
    "@types/enzyme": "^3.1.10",
    "@types/jest": "^22.2.3",
    "@types/js-cookie": "^2.1.0",
    "@types/jschannel": "^1.0.0",
    "@types/jsonp": "^0.2.0",
    "@types/lodash": "^4.14.111",
    "@types/node": "^9.6.6",
    "@types/query-string": "^5.1.0",
    "@types/react": "^16.3.12",
    "@types/react-color": "^2.13.5",
    "@types/react-dom": "^16.0.5",
    "@types/react-redux": "^6.0.0",
    "@types/react-router-dom": "^4.2.6",
    "@types/redux-mock-store": "0.0.13",
    "@types/tinycolor2": "^1.4.1",
    "@types/uuid": "^3.4.3",
    "axios-mock-adapter": "^1.15.0",
    "enzyme": "^3.4.0",
    "enzyme-adapter-react-16": "^1.2.0",
    "enzyme-to-json": "^3.3.4",
    "eslint": "^4.19.1",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.10.0",
    "pattern-lab": "git+ssh://git@github.com/bigcommerce/pattern-lab.git#v3.0.0-alpha.20",
    "react-color": "^2.14.1",
    "redux-mock-store": "^1.5.3",
    "srcdoc-polyfill": "git+https://github.com/jugglinmike/srcdoc-polyfill.git",
    "typescript": "^2.9.2",
    "typescript-eslint-parser": "^16.0.0",
    "window": "^4.2.5"
